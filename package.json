  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "build:fast": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 15",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf build dist node_modules/.vite",
    "deps:update": "npm update && npm audit fix",
    "size:analyze": "npm run build:analyze",
    "perf:lighthouse": "npx lighthouse http://localhost:3001 --output json --output html --output-path ./lighthouse-report",
    "check:all": "npm run lint && npm run type-check && npm run test:coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
